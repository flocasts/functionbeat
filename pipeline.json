{
  "description": "CloudWatch Logs Pipeline",
  "processors": [
    {
      "date": {
        "ignore_failure": false,
        "field": "log.context.requestContext.requestTimeEpoch",
        "target_field": "@timestamp",
        "formats": ["UNIX_MS"],
        "on_failure": [
          {
            "set" : {
              "field": "error",
              "value": "{{ _ingest.on_failure_message }}"
            }
          }
        ]
      }
    }
  ]
}