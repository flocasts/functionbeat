{
  "cloudwatch-logs" : {
    "description" : "CloudWatch Logs Pipeline.",
    "processors" : [
      {
        "date" : {
          "ignore_failure" : false,
          "field" : "start_time",
          "formats" : ["yyyy-MM-dd'T'HH:mm:ss.SSS'Z'"],
          "on_failure" : [
            {
              "set" : {
                "field" : "ingest_error",
                "value" : "{{ _ingest.on_failure_message }}"
              }
            }
          ]
        }
      }
    ]
  }
}
