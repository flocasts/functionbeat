{
  "cloudwatch-logs" : {
    "description" : "CloudWatch Logs API Gateway Pipeline.",
    "processors" : [
      {
        "date" : {
          "ignore_failure" : false,
          "field" : "requestTime",
          "formats" : ["UNIX_MS"],
          "on_failure" : [
            {
              "set" : {
                "field" : "ingest_error",
                "value" : "{{ _ingest.on_failure_message }}"
              }
            }
          ]
        }
      }
    ]
  }
}